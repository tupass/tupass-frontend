        <source>Enter a password to evaluate</source>
        <target>Gib ein Passwort zur Evaluation ein</target>
      </trans-unit>
      <trans-unit id="tooLongInput" datatype="html">
        <source>Your input is too long</source>
        <target>Die Eingabe ist zu lang</target>
      </trans-unit>
      <trans-unit id="nonASCIICharsInside" datatype="html">
        <source>
          Please remove the following non-ASCII-characters from your input: <x id="INTERPOLATION" equiv-text="{{ getFaultyCharacters() }}"/>
        </source>
        <target>
          Bitte löschen Sie die folgenden nicht-ASCII-Zeichen aus der Eingabe: <x id="INTERPOLATION" equiv-text="{{ getFaultyCharacters() }}"/>
        </target>
      </trans-unit>
      <trans-unit id="invalidInput" datatype="html">
